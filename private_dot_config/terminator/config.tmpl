[global_config]
  focus = system
  window_state = maximise
  title_hide_sizetext = True
  title_transmit_fg_color = "#a3be8c"
  title_transmit_bg_color = "#2b303b"
  title_inactive_fg_color = "#c0c5ce"
  title_inactive_bg_color = "#2b303b"
  enabled_plugins = ActivityWatch, TerminalShot, LaunchpadCodeURLHandler, Logger, LaunchpadBugURLHandler
[keybindings]
  toggle_zoom = <Shift><Alt>Return
[profiles]
  [[default]]
    background_color = "#2b303b"
    cursor_color = "#c0c5ce"
    font = Hack Nerd Font 14
    foreground_color = "#c0c5ce"
    scrollback_infinite = True
    palette = "#2b303b:#bf616a:#a3be8c:#ebcb8b:#8fa1b3:#b48ead:#96b5b4:#c0c5ce:#65737e:#bf616a:#a3be8c:#ebcb8b:#8fa1b3:#b48ead:#96b5b4:#eff1f5"
    use_system_font = False
    copy_on_selection = True
  [[New Profile]]
    background_color = "#fbf1c7"
    cursor_color = "#aaaaaa"
    foreground_color = "#3c3836"
    palette = "#fbf1c7:#cc241d:#98971a:#d79921:#458588:#b16286:#689d6a:#7c6f64:#928374:#9d0006:#79740e:#b57614:#076678:#8f3f71:#427b58:#3c3836"
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = Base16 Ocean Dark
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
